{
    "name": "fints",
    "version": "0.3.1",
    "description": "FinTS client library.",
    "keywords": [
        "fints",
        "hbci",
        "banking",
        "api"
    ],
    "scripts": {
        "prebuild": "cxsd -t generated https://raw.githubusercontent.com/hbci4j/hbci4java/master/src/main/resources/pain.001.001.03.xsd",
        "build": "tsc -p ./tsconfig-build.json",
        "clean": "rm -rf dist",
        "docs": "typedoc --mode file --out docs/ src/",
        "test": "TZ=UTC jest"
    },
    "files": [
        "dist",
        "LICENSE",
        "package.json",
        "README.md"
    ],
    "main": "dist/index.js",
    "repository": "https://github.com/Prior99/fints",
    "typings": "dist/index.d.ts",
    "author": "Frederick Gnodtke",
    "license": "MIT",
    "jest": {
        "collectCoverage": true,
        "moduleFileExtensions": [
            "ts",
            "js"
        ],
        "transform": {
            ".ts": "ts-jest"
        },
        "coverageReporters": [
            "lcov",
            "text-summary"
        ],
        "testMatch": [
            "**/__tests__/test-*.ts"
        ],
        "globals": {
            "ts-jest": {
                "enableTsDiagnostics": true,
                "ignoreCoverageForDecorators": true,
                "ignoreCoverageForAllDecorators": true
            }
        },
        "coveragePathIgnorePatterns": [
            "/node_modules/",
            "/__tests__/"
        ],
        "collectCoverageFrom": [
            "src/**/*.ts"
        ]
    },
    "devDependencies": {
        "@types/date-fns": "^2.6.0",
        "@types/fetch-mock": "^6.0.3",
        "@types/iconv-lite": "^0.0.1",
        "@types/invariant": "^2.2.29",
        "@types/isomorphic-fetch": "^0.0.34",
        "@types/jest": "^23.3.1",
        "@types/node": "^10.9.4",
        "@types/winston": "^2.4.4",
        "cxsd": "^0.1.1",
        "fetch-mock": "^7.0.0-alpha.6",
        "jest": "^23.5.0",
        "ts-jest": "^23.1.4",
        "tslint": "^5.11.0",
        "typedoc": "^0.12.0",
        "typescript": "^3.0.3"
    },
    "dependencies": {
        "bind-decorator": "^1.0.11",
        "date-fns": "^1.29.0",
        "fast-xml-parser": "^3.12.12",
        "iconv-lite": "^0.4.24",
        "invariant": "^2.2.4",
        "isomorphic-fetch": "^2.2.1",
        "mt940-js": "^0.6.0",
        "winston": "^3.1.0"
    }
}
